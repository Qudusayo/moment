{
  "name": "Moment",
  "description": "Moment Chrome extension",
  "version": "1.0",
  "manifest_version": 3,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Video Moment"
  },
  "permissions": ["activeTab", "scripting"]
}
